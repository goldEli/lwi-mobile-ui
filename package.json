{
  "name": "lwj-mobile-ui",
  "version": "0.0.2",
  "browser": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "description": "lwj mobile ui",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "yarn run dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "dev:rollup-h5": "rollup --config ./rollup-h5.config.js -w",
    "build": "yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json --declaration --declarationDir types",
    "build:rollup": "rollup --config ./rollup.config.js",
    "build:rollup-h5": "rollup --config ./rollup-h5.config.js",
    "prepublishOnly": "yarn run clean && yarn run build",
    "lint": "eslint ./src --fix",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:style-fix": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "test": "cross-env NODE_ENV=test && jest --coverage",
    "test:ci": "yarn run build:h5 && yarn run test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf .temp dist lib coverage",
    "storybook": "yarn build:rollup-h5 && start-storybook -p 6006 -s public",
    "build-storybook": "yarn build:rollup-h5 && build-storybook"
  },
  "keywords": [
    "taro"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/react": "6.0.28",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.x",
    "@typescript-eslint/parser": "^2.x",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-taro": "^3.0.8",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "26",
    "rimraf": "3.0.2",
    "rollup": "^2.16.1",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-visualizer": "^4.0.4",
    "stylelint": "13.6.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.17.2",
    "ts-jest": "26",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0"
  },
  "dependencies": {
    "@tarojs/components": "^3.0.8",
    "@tarojs/react": "^3.0.8",
    "taro-ui": "3.0.0-alpha.10"
  }
}
